/* laber */
.index-laber{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 16rpx 0;
  background-color: #ffffff;
  border-radius: 10rpx;
}
.laber-item{
  display: flex;
  align-items: center;
}
.laber-item image{
  width: 32rpx;
  height: 32rpx;
}
.laber-item text{
  font-size: 22rpx;
  color: #666666;
  padding-left: 8rpx;
}